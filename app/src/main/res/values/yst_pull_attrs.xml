<?xml version="1.0" encoding="UTF-8"?>
<resources>
    
    <!-- refresh_pull RefreshBase.java use it -->
    <declare-styleable name="yst_PullToRefresh">
        <attr name="yst_adapterViewBackground" format="reference|color" />
        <attr name="yst_headerBackground" format="reference|color" />
        <attr name="yst_headerTextColor" format="color" />
        <attr name="yst_mode">
            <flag name="pullDownFromTop" value="0x1" />
            <flag name="pullUpFromBottom" value="0x2" />
            <flag name="both" value="0x3" />
        </attr>
    </declare-styleable>
     
</resources>

